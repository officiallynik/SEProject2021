{
  "name": "easyso",
  "displayName": "EasySO",
  "description": "",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git"
  },
  "engines": {
    "vscode": "^1.37.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.searchWithTextBox",
    "onCommand:extension.searchTerminalErrors",
    "onCommand:extension.searchWithAutoQuery"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.searchWithTextBox",
        "title": "Search",
        "category": "EasySO"
      },
      {
        "command": "extension.searchTerminalErrors",
        "title": "Search Terminal Errors",
        "category": "EasySO"
      },
      {
        "command": "extension.searchWithAutoQuery",
        "title": "Search with AutoQuery",
        "category": "EasySO"
      }
    ],
    "keybindings": [
      {
        "command": "extension.searchWithTextBox",
        "key": "ctrl+alt+s",
        "mac": "cmd+alt+s"
      },
      {
        "command": "extension.searchTerminalErrors",
        "key": "ctrl+alt+t",
        "mac": "cmd+alt+t"
      },
      {
        "command": "extension.searchWithAutoQuery",
        "key": "ctrl+alt+a",
        "mac": "cmd+alt+a"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.37.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "ts-loader": "^6.0.4",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode-test": "^1.0.0-next.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7"
  }
}
